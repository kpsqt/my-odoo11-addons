        <record model="ir.ui.view" id="svn_checkout_wizard_form_view">
            <field name="name">svn_checkout_wizard.form</field>
            <field name="model">c1_project_dev.svn_checkout_wizard</field>
            <field name="arch" type="xml">
                <form string="Svn checkout wizard">
                    <h2>Svn checkout Wizard</h2>
                    <p>
                        This wizard allows you to checkout the module from the development server to user's personal
                        addons path.
                        <br/>
                        <strong>You may fill the form with accurate svn information for the operation to be
                            successful.
                        </strong>
                    </p>
                    <newline/>
                    <group string="Checkout info">
                        <field name="svn_repository_id" options="{'no_create':True}"/>
                        <field name="module_id" options="{'no_create':True}"/>
                        <!--<field name="name" />-->
                        <field name="rev"/>
                        <!--<field name="svn_account" readonly="1"/>-->
                        <!--<field name="svn_password" password="1" invisible="1"/>-->
                    </group>
                    <group string="Destination">
                        <field name="destination" readonly="1"/>
                    </group>
                    <group>
                        <field name="restart_container"/>
                    </group>
                    <footer>
                        <div class="oe_button_box oe_right">

                            <button type="object" icon="fa-download" class="oe_highlight" string="Checkout"
                                    name="button_checkout"/>
                            <button type="special" name="cancel" string="Cancel"/>
                        </div>
                    </footer>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="cmd_wizard_form_view">
            <field name="name">c1_project_dev.linux_cmd.wizard.form</field>
            <field name="model">c1_project_dev.linux_cmd.wizard</field>
            <field name="arch" type="xml">
                <form string="Linux command run">
                    <h2>Linux command running wizard</h2>
                    <p>
                        Not all kind of command can successfully run.
                    </p>
                    <group>
                        <field name="state" options="{'horizontal': true}" widget="radio"/>
                    </group>
                    <notebook>
                        <page states="single" string="Running single command">
                            <p></p>
                            <div>
                                <label for="command_id"/>
                                <field options="{'no_create':True}" class="oe_inline"
                                       attrs="{'required':[('state','=','single')]}" name="command_id"/>
                                <field width="50%"
                                       attrs="{'invisible':[('cmd_type','!=','with_parameter'), ('state','=','single')], 'required':[('cmd_args_mandatory','=',True)]}"
                                       class="oe_inline" name="cmd_args"/>
                                -
                                <button type="object" icon="fa-play" class="oe_inline oe_highlight" string="Run"
                                        name="execute"/>
                            </div>
                            <field name="cmd_description"/>
                            <group>
                                <field name="cmd_type" invisible="1"/>
                                <field name="cmd_args_mandatory" invisible="1"/>
                                <field name="cmd_preview"/>
                            </group>
                        </page>
                        <page states="multiple" string="Running multiple command">
                            <group>
                                <field name="cmd_line_ids" nolabel="1">
                                    <tree editable="bottom">
                                        <field name="sequence" widget="handle"/>
                                        <field name="cmd_type" invisible="1"/>
                                        <field name="cmd_args_mandatory" invisible="1"/>
                                        <field name="command_id" options="{'no_create':True}"/>
                                        <field name="cmd_args"
                                               attrs="{'invisible':[('cmd_type','!=','with_parameter')],'required':[('cmd_args_mandatory','=',True)]}"/>
                                    </tree>
                                </field>
                            </group>
                            <group class="oe_subtotal_footer">
                                <button name="execute_multiple" string="Run" type="object" class="oe_highlight"
                                        icon="fa-list"/>
                            </group>
                        </page>
                    </notebook>

                    <group string="Output">
                        <field name="output" nolabel="1"/>
                    </group>

                    <footer>
                        <div class="oe_button_box oe_right">
                            <button type="special" name="cancel" string="Close"/>
                        </div>
                    </footer>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="module_dep_wizard_form_view">
            <field name="name">c1_project_dev.module_dependencies.wizard.form</field>
            <field name="model">c1_project_dev.module_dependencies.wizard</field>
            <field name="arch" type="xml">
                <form string="Module dependences checking">
                    <group>
                        <field name="module_id" options="{'no_create':True}"/>
                        <field name="satisfied" attrs="{'invisible':[('module_id','=',False)]}"/>
                    </group>
                    <group string="Dependencies">
                        <field name="dependencies" nolabel="1">
                            <tree editable="bottom" edit="1" create="0" delete="0" decoration-success="installed==True"
                                  decoration-danger="installed!=True">
                                <field name="sequence" widget="handler" default_order="sequence" invisible="1"/>
                                <field name="name" readonly="1"/>
                                <field name="installed_version" readonly="1"/>
                                <field name="installed" readonly="1"/>
                                <field name="install_method" widget="selection"
                                       attrs="{'invisible':[('installed','=',True)]}"/>
                                <field name="to_install" attrs="{'invisible':[('installed','=',True)]}"/>
                                <!--<button name="button_install" type="object" string="Install" />-->
                            </tree>
                        </field>
                    </group>
                    <footer>
                        <div class="oe_button_box oe_right">
                            <button type="special" name="cancel" string="Close"/>
                            <button attrs="{'invisible':['|',('module_id','=',False),('satisfied','=',True)]}"
                                    type="object" icon="fa-download" name="button_install_dependencies"
                                    class="oe_highlight" string="Install dependencies"/>
                        </div>
                    </footer>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="wizard_change_svn_view">
            <field name="name">res.users.svn.wizard.form</field>
            <field name="model">res.users.svn.wizard</field>
            <field name="arch" type="xml">
                <form string="Change svn credentials">
                    <h2>Changing your svn credentials</h2>
                    <group>
                        <field name="current_user_password" password="1"/>
                        <field name="svn_account"/>
                        <field name="svn_password" password="1"/>
                    </group>
                    <footer>
                        <div class="oe_button_box oe_right">
                            <button type="object" class="oe_highlight" string="Change" name="change_svn_credentials"/>
                            <button type="special" name="cancel" string="Cancel"/>
                        </div>
                    </footer>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="wizard_db_manage_view">
            <field name="name">c1_project_dev.database.wizard.form_view</field>
            <field name="model">c1_project_dev.database.wizard</field>
            <field name="arch" type="xml">
                <form string="Manage Database">
                    <h2>Database management wizard</h2>
                    <field name="action" widget="radio" options="{'horizontal':True}"/>
                    <group string="Manage database" attrs="{'invisible':[('action','!=','maintain')]}">
                        <field name="database_info_id" options="{'no_create':True}"/>
                        <field name="delete_backup_files"/>
                        <div>
                            <!--<field name="db_copy_name" attrs="{'invisible':[('database_info_id','=',False)]}"/>-->
                        </div>
                        <div class="oe_button_box">
                            <button style="margin-right:5px;" attrs="{'invisible':[('database_info_id','=',False)]}"
                                    type="object" icon="fa-save" class="oe_highlight" string="Backup"
                                    name="button_backup_database"/>
                            <!--<button attrs="{'invisible':[('database_info_id','=',False)]}" type="object" icon="fa-copy" class="oe_highlight" string="Duplicate" name="button_duplicate_database" />-->
                            <button style="margin-right:5px;" attrs="{'invisible':[('database_info_id','=',False)]}"
                                    type="object" icon="fa-trash" class="oe_highlight" string="Delete"
                                    confirm="Do you really want to delete this database?" name="button_drop_database"/>
                        </div>
                    </group>
                    <group string="Create a new database" attrs="{'invisible':[('action','!=','create')]}">
                        <label for="db_name" string="Database name"/>
                        <div>
                            <field name="db_name" attrs="{'required':[('action','=','create')]}"
                                   help="To this name will be appended the database suffix of the current user"
                                   placeholder="e.g.: demo"/>
                        </div>
                        <field name="login" attrs="{'required':[('action','=','create')]}"/>
                        <field name="passwd" attrs="{'required':[('action','=','create')]}"/>
                        <field name="country"/>
                        <field name="lang"/>
                        <field name="demo"/>
                        <button type="object" icon="fa-save" class="oe_highlight" string="Create database"
                                name="button_create_database"/>
                    </group>
                    <group string="Restore database" attrs="{'invisible':[('action','!=','restore')]}">
                        <field name="recovery_type" attrs="{'required':[('action','=','restore')]}"/>
                        <field name="server"
                               attrs="{'invisible':[('recovery_type','!=','production_server')], 'required':[('action','=','restore'),('recovery_type','=','production_server')]}"/>
                        <field name="db_project_record" domain="[('type','=','folder')]" options="{'no_create':True}"
                               attrs="{'required':[('action','=','restore'),('recovery_type','=','production_server')],'invisible':[('recovery_type','!=','production_server')]}"/>
                        <field name="database_record_id" widget="selection"
                               domain="[('type','=','folder'), ('source','=','user')]" options="{'no_create':True}"
                               attrs="{'required':[('action','=','restore'),('recovery_type','=','personal_path')],'invisible':[('recovery_type','!=','personal_path')]}"/>
                        <!--<field name="db_backup_file" domain="[('type','=','file')]" options="{'no_create':True}" attrs="{'required':[('action','=','restore'),('recovery_type','=','production_server')],'invisible':[('recovery_type','!=','production_server')]}"/>-->
                        <field name="db_backup_file" domain="[('type','=','file')]" options="{'no_create':True}"
                               attrs="{'required':[('action','=','restore')]}"/>
                        <!--<field name="perso_db_backup_file" attrs="{'required':[('action','=','restore'),('recovery_type','=','personal_path')],'invisible':[('recovery_type','!=','personal_path')]}"/>-->
                        <label for="backup_db_name" string="Database name"/>
                        <div>
                            <field name="backup_db_name" options="{'no_create':True}"
                                   attrs="{'required':[('action','=','restore')]}"/>
                        </div>
                        <field name="login_restore"/>
                        <field name="passwd_restore"/>
                        <button type="object" icon="fa-recycle" class="oe_highlight" string="Restore"
                                name="button_restore_database"/>
                    </group>
                    <footer>
                        <button type="special" name="cancel" string="Cancel"/>
                    </footer>
                </form>
            </field>
        </record>


        <record model="ir.ui.view" id="wizard_module_install_view">
            <field name="name">c1_project_dev.module.wizard.form_view</field>
            <field name="model">c1_project_dev.module.wizard</field>
            <field name="arch" type="xml">
                <form string="Module install">
                    <h2>Module installation wizard</h2>
                    <group string="Module Info">
                        <field name="module_id"/>
                        <field name="current_downloaded_rev"/>
                        <field name="installable"/>
                        <field name="repository_id" readonly="1"/>
                        <field name="on_server_rev" readonly="1"/>
                        <field name="update" attrs="{'invisible':[('current_downloaded_rev','=','on_server_rev')]}"/>
                        <field name="update_rev" attrs="{'invisible':[('update','!=',True)]}"/>
                    </group>
                    <group string="Database info">
                        <field string="Install in" name="database_id" required="1"/>
                        <field name="login" required="1"/>
                        <field name="passwd" password="1" required="1"/>
                    </group>
                    <footer>
                        <button type="object" icon="fa-download" class="oe_highlight" string="Install"
                                name="button_install"/>
                        <button type="special" name="cancel" string="Cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <record model="ir.ui.view" id="user_addons_path_wizard_form_view">
            <field name="name">c1_project_dev.user_addons_path.wizard.form</field>
            <field name="model">c1_project_dev.user_addons_path.wizard</field>
            <field name="arch" type="xml">
                <form string="Module dependences checking">
                    <div class="oe_button_box">
                        <button class="oe_stat_button" type="object" name="button_svn_checkout" icon="fa-download">
                            <span>Svn checkout</span>
                        </button>
                        <button class="oe_stat_button" type="object" name="button_checkout" icon="fa-download">
                            <span>Checkout</span>
                        </button>
                    </div>
                    <h1>
                        <field name="user_id" readonly="1"/>
                    </h1>
                    <group string="Filter">
                        <field name="filter_technical_name" class="oe_inline"/>
                        <button type="object" name="button_filter" string="filter" class="oe_highlight"
                                icon="fa-filter"/>
                    </group>

                    <!--<h2><field name=""/></h2>-->
                    <group string="Addons">
                        <field name="module_info_ids" nolabel="1">
                            <tree edit="0" create="0" decoration-danger="server_rev > current_rev" delete="0">
                                <field name="name"/>
                                <field name="is_svn"/>
                                <field name="current_rev"/>
                                <field name="server_rev"/>
                                <field name="rev_author"/>
                                <field name="last_checkout_time"/>
                                <button name="button_refresh" string="Refresh" type="object" icon="fa-refresh"/>
                                <button name="button_delete" confirm="Do you really want to delete this addon?"
                                        string="Delete" type="object" icon="fa-remove"/>
                            </tree>
                        </field>
                    </group>
                </form>
            </field>
        </record>


        <record model="ir.ui.view" id="docker_manager_wizard_form_view">
